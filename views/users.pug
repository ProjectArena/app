extends layout
block styles
  link(rel='stylesheet', type='text/css', href='/css/users.css' media='screen')
block content
    .container
        each item in user
            .user.col-md-4.col-md-offset-3.border
                p ID:           #{item.dataValues.id}
                p First Name:   #{item.dataValues.firstName}
                p Last Name:    #{item.dataValues.lastName}
                p Username:     #{item.dataValues.username}